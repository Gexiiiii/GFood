<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemView">
        <attr name="title" format="string" />
        <attr name="title_style" format="reference" />
        <attr name="icon_left" format="reference" />
        <attr name="icon_right" format="reference" />
        <attr name="background" format="reference" />
        <attr name="isShowUnderLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="imgHeight" format="integer" />
        <attr name="textColor" format="color" />
        <attr name="textTitleColor" format="color" />
        <attr name="textLeft" format="string" />
        <attr name="textCenter" format="string" />
        <attr name="textRight" format="string" />
        <attr name="imgLeft" format="reference" />
        <attr name="imgRight" format="reference" />
    </declare-styleable>

    <declare-styleable name="GEditText">
        <attr name="etIcon" format="reference" />
        <attr name="etInputType" format="enum">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="num" value="2" />
        </attr>
        <attr name="etMaxLength" format="integer" />
        <attr name="etTitle" format="string" />
        <attr name="etHint" format="string" />
        <attr name="etIsShowUnderLine" format="boolean" />
    </declare-styleable>
</resources>
